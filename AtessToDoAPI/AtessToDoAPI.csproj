    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>1a69f1cf-a406-4d11-9aa2-ff37eba2e625</UserSecretsId>
    <DockerDefaultTargetOS>Windows</DockerDefaultTargetOS>
